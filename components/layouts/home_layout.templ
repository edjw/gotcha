package layouts

templ HomeLayout(pageName string) {
	// This is the layout for the home page
	@BaseLayout(pageName) {
		<header>
			<h1 class="text-2xl">{ pageName }</h1>
		</header>
		<main class="flex flex-col prose gap-y-4">
			{ children... }
		</main>
		<footer>
			<p>Footer content</p>
		</footer>
	}
}
